{
  "name": "stylus-skeuomorphic",
  "version": "0.4.0",
  "description": "skeuomorphic UI patrs generated by stylus.",
  "main": "stylus-skeuomorphic.js",
  "scripts": {
    "help": "npm-run-help",
    "build:css": "stylus -u nib examples/**/main.styl",
    "watch:css": "stylus -w -u nib examples/**/main.styl",
    "build": "npm-run-all build:*",
    "watch": "npm-run-all watch:*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/all-user/stylus-skeuomorphic.git"
  },
  "author": "Keita Okamoto<ok@aaa.email.ne.jp>",
  "bugs": {
    "url": "https://github.com/all-user/stylus-skeuomorphic/issues"
  },
  "keywords": [
    "stylus",
    "skeuomorphic",
    "skeu",
    "css",
    "box-shadow",
    "text-shadow",
    "GUI",
    "UI",
    "nib"
  ],
  "homepage": "https://github.com/all-user/stylus-skeuomorphic",
  "dependencies": {
    "folder-contents": "^1.1.0",
    "nib": "^1.1.0",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "npm-run-all": "^2.2.2",
    "npm-run-help": "^1.1.0"
  },
  "license": "MIT"
}
